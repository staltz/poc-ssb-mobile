    "react-native-udp": "^1.2.9",
    "readable-stream": "2.2.9",
    "rn-nodeify": "7.0.1",
    "scuttlebot": "9.8.0",
    "sodium-browserify": "1.2.1",
    "sodium-browserify-tweetnacl": "0.2.3",
    "ssb-config-rn": "file:./ssb-config-rn",
    "ssb-ws": "1.0.1",
    "stream-browserify": "2.0.1",
    "stream-scarecrow": "file:./stream-scarecrow",
    "timers-browserify": "1.4.2",
    "tty-browserify": "0.0.0",
    "url": "0.10.3",
    "vm-browserify": "0.0.4"
