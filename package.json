{
  "name": "ssbmobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "rn-nodeify": "rn-nodeify --install fs,dgram,process,path,console --hack",
    "fix-shims": "node fix-shims.js",
    "patch-dependencies": "node patch-dependencies.js",
    "postinstall": "patch-package && npm run fix-shims && npm run rn-nodeify && npm run patch-dependencies",
    "android": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
