  "react-native": {
    "asyncstorage-down": "@staltz/asyncstorage-down",
    "console": "console-browserify",
    "path": "path-browserify",
    "crypto": "react-native-crypto",
    "stream": "stream-browserify",
    "osenv": false,
    "chloride": "sodium-browserify-tweetnacl",
